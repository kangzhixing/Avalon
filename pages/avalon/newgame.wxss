.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    margin: 5px 3px;
    width:20%;
    justify-content: center;
    text-align: center;
}
.td image{
  width:100%;
  height:90px;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}

.db{
  display: block;
}

.dn{
  display: none;
}

.btnGetRole{
  color: white;
  width: 160px;
  background-color: #34A853;
}

.btnStart {
  width: 160px;
  margin-top: 10px;
  background-color: lightgrey;
}